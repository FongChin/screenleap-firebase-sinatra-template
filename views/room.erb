<!DOCTYPE HTML>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>A simple static page</title>
	<link rel="stylesheet" href="stylesheets/bootstrap.min.css" />
	<script type="text/javascript" src="javascripts/jquery1.7.1.js"></script>
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
  <script src="http://www.screenleap.com/js/screenleap-api.js" type="text/javascript"></script>
  <script src="https://cdn.firebase.com/v0/firebase.js"></script>
  <script type="text/javascript" src="javascripts/bootstrap.min.js"></script>
</head>
<body>
  <div id="alertMsgDiv" style="display: none"></div>
  <div id="stopButtonContainer" style="display: none">
    <button class="btn btn-primary" id="stopScreenSharebtn">Stop Sharing</button>
  </div>
  <div id="shareButtonContainer">
    <button class="btn btn-primary" id="startScreenSharebtn">Share Screen</button>
  </div>
  <div id="screenleapExtensionProperties" style="display: none"></div>
  <div id="iframeDiv"></div>

  <script type="text/javascript">
    firebaseURL = "<%= @firebase %>";
    roomId = "<%= @roomId %>";
  </script>
  <script src="javascripts/app.js" type="text/javascript"></script>
</body>
</html>

